# get yaml for 2 level   
def get_entry (parsed_yaml, entry, param):
    deps_list = parsed_yaml.get(entry)
    param_list = [] 
    count = 0
    for i in range(0,len(deps_list)):
        if deps_list[i].get(param) is not None:
            param_list.append(deps_list[i].get(param))
            count += 1
        else:
            continue
    if count != 0:
        return param_list
    else:
        return False